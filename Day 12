<script>
    //set->can't accept duplicates to uniquly identify the values in the set
    //cerate a set
    let email=new Set([]);
    email.add("ak@gmail.com");
    email.add("java@gmail.com");
    email.add("python@gmail.com");
    console.log(email);
    email.delete("js@gmail.com");
    console.log(email);

    for(let item of email){
        console.log(item);
    }
<
    email.forEach(x =>{
        console.log(x);
    });
    //example
    let adminpermission=new Set(["read","write","delete"]);
    let userpermission=new Set(["read"]);

    function hasPermission(role,permission){
        return role.has(permission);
    }
    console.log(hasPermission(adminpermission,"delete"));
    console.log(hasPermission(userpermission,"delete"));
    //map
    let user1={name:"akshaya"};
    let user2={name:"danya"};
    let map=new Map();
    map.set(user1,"welcome");
    map.set(user2,"hello");
    console.log(map);
    map.delete(user2);
    console.log(map);
    for(let details of map.keys){
        console.log(details);
    }
</script>
